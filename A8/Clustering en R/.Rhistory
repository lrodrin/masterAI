km_clusters <- kmeans(BreastCancer.features, centers = 2, nstart = 20)
km_clusters
km_clusters$centers
aggregate(BreastCancer, by = list(cluster = km_clusters$cluster), mean)
aggregate(BreastCancer[, 1:9], by = list(cluster = km_clusters$cluster), mean)
aggregate(BreastCancer.features[, 1:9], by = list(cluster = km_clusters$cluster), mean)
